{
  "name": "stevenbuehner/sb-churchtools-api",
  "description": "ChurchTools Api Wrapper",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Steven Buehner",
      "email": "buehner@me.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "StevenBuehner\\ChurchTools\\": "src/lib",
      "StevenBuehner\\ChurchToolsApi\\": "src/ChurchToolsApi"
    }
  },
  "archive": {
    "exclude": [
      "/material/*",
      "/src/docs",
      "/src/test",
      "/src/.travis.yml",
      "/src/.openapi-generator",
      "/src/.php_cs",
      "/src/git_push.sh",
      "/src/phpunit.xml.dist",
      "/src/README.md"
    ]
  }
}
