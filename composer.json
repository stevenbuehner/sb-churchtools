{
  "name": "stevenbuehner/sb-churchtools-api",
  "description": "ChurchTools Api Wrapper",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Steven Buehner",
      "email": "buehner@me.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "StevenBuehner\\ChurchTools\\": "src/lib",
      "StevenBuehner\\ChurchToolsApi\\": "src/ChurchToolsApi"
    }
  },
  "autoload-dev": {
    "psr-4": { "StevenBuehner\\ChurchTools\\Test\\" : "src/test/" }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  }
}
